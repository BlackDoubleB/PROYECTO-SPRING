$(document).ready(function() {
	habilitarBtnIngresar()
});

function habilitarBtnIngresar(){
	$("#btn-ingresar").click(function(e){
		procesarLogin(e)
	})
}

function procesarLogin(e){
	e.preventDefault()
	
	console.log("evento login")
}

